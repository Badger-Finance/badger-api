{
  "name": "badger-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "sls offline start --useChildProcesses",
    "dev:api": "yarn dev --disableScheduledEvents",
    "lint": "eslint '**/*.{ts,js}'",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand --logHeapUsage --detectOpenHandles --detectLeaks",
    "test:coverage": "yarn run test:unit --coverage",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install",
    "codegen:graphql": "graphql-codegen -r dotenv/config -c codegen.yml",
    "codegen:contracts": "typechain --target ethers-v5 --out-dir src/contracts src/abi/*.abi"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@badger-dao/sdk": "^1.17.2",
    "@tsed/ajv": "^6.126.1",
    "@tsed/common": "^6.126.1",
    "@tsed/core": "^6.126.1",
    "@tsed/di": "^6.126.1",
    "@tsed/engines": "^6.126.1",
    "@tsed/exceptions": "^6.126.1",
    "@tsed/json-mapper": "^6.126.1",
    "@tsed/logger": "^6.2.1",
    "@tsed/logger-file": "^6.2.1",
    "@tsed/platform-cache": "^6.126.1",
    "@tsed/platform-exceptions": "^6.126.1",
    "@tsed/platform-express": "^6.126.1",
    "@tsed/platform-log-middleware": "^6.126.1",
    "@tsed/platform-middlewares": "^6.126.1",
    "@tsed/platform-params": "^6.126.1",
    "@tsed/platform-response-filter": "^6.126.1",
    "@tsed/platform-serverless": "^6.126.1",
    "@tsed/platform-serverless-http": "^6.126.1",
    "@tsed/platform-views": "^6.126.1",
    "@tsed/schema": "^6.126.1",
    "@tsed/swagger": "^6.126.1",
    "ajv": "^8.11.0",
    "barrelsby": "^2.4.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "method-override": "^3.0.0",
    "serverless": "^3.21.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-http": "^3.0.2",
    "serverless-offline": "^9.2.0",
    "serverless-plugin-typescript": "^2.1.2"
  },
  "devDependencies": {
    "@badger-dao/sdk-mocks": "^1.17.1",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@tsed/cli-plugin-eslint": "3.24.0",
    "@tsed/cli-plugin-jest": "3.24.0",
    "@tsed/platform-serverless-testing": "^6.126.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/aws-lambda": "^8.10.102",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "find-my-way": "^7.0.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-create-mock-instance": "2.0.0",
    "jest-mock-extended": "2.0.6",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
