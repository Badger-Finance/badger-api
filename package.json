    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "supertest": "6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.2",
    "typescript": "^4.2.4"
  }
}
