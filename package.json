{
  "name": "badger-api",
  "version": "2.0.0",
  "description": "Badger Serverless API",
  "repository": "git@github.com:BadgerFinance/badger-api.git",
  "author": "Badger Finance",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./build",
    "codegen:graphql": "graphql-codegen -r dotenv/config -c codegen.yml",
    "codegen:contracts": "typechain --target ethers-v5 --out-dir src/contracts src/abi/*.abi",
    "deploy": "sls deploy",
    "deploy:prod": "sls deploy --stage prod",
    "dev": "sls offline start",
    "dev:local": "npx ts-node src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "tsc:build": "tsc -p ."
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@badger-dao/sdk": "^1.0.44",
    "@tsed/common": "6.53.0",
    "@tsed/core": "6.53.0",
    "@tsed/di": "6.53.0",
    "@tsed/exceptions": "6.53.0",
    "@tsed/json-mapper": "6.53.0",
    "@tsed/platform-aws": "6.53.0",
    "@tsed/platform-express": "6.53.0",
    "@tsed/schema": "6.53.0",
    "@tsed/swagger": "6.53.0",
    "@types/cors": "^2.8.10",
    "aws-sdk": "^2.913.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ethers": "^5.2.0",
    "globby": "^11.0.3",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.4",
    "method-override": "^3.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "serverless-dynamodb-client": "^0.0.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-graphql-request": "3.2.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonfile": "^6.0.0",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "@types/supertest": "2.0.11",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "dotenv": "10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "ethers-typescript-typings": "^0.0.4",
    "express": "4",
